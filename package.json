{
 "name": "@tucows/donejs-carousel-plugin",
 "version": "3.0.0",
 "description": "A carousel plugin for donejs, using donejs@2, canjs@4",
 "homepage": "https://github.com/tucows/donejs-carousel-plugin",
 "repository": {
  "type": "git",
  "url": "git://github.com/Tucows/donejs-carousel-plugin.git"
 },
 "author": "Ranjan Subbiah <rsubbiah@tucows.com>",
 "contributors": [
  {
   "name": "Paul Tichonczuk",
   "email": "junk@tracer.ca",
   "url": "http://tracer.ca"
  }
 ],
 "scripts": {
  "preversion": "npm test && npm run build",
  "version": "git commit -am \"Update version number\" && git checkout -b release && git add -f dist/",
  "postpublish": "git push --tags && git checkout master && git branch -D release && git push",
  "test": "npm run eslint && npm run testee",
  "test_debug": "DEBUG='testee:*' testee ./test.html --config testee.json",
  "testee": "testee ./test.html --config testee.json",
  "eslint": "eslint --fix 'src/**/*.js'",
  "release:patch": "npm version patch && npm publish",
  "release:minor": "npm version minor && npm publish --access public",
  "release:major": "npm version major && npm publish --access public",
  "build": "node build.js",
  "develop": "done-serve --static --develop --port 8080"
 },
 "main": "@tucows/donejs-carousel-plugin",
 "files": [
  "src"
 ],
 "browser": {
  "transform": [
   "cssify"
  ]
 },
 "browserify": {
  "transform": [
   "cssify"
  ]
 },
 "keywords": [
  "donejs-plugin"
 ],
 "steal": {
  "main": "donejs-carousel-plugin",
  "configDependencies": [
   "live-reload"
  ],
  "npmIgnore": [
   "testee",
   "generator-donejs",
   "donejs-cli",
   "steal-tools"
  ],
  "plugins": [
   "steal-less",
   "steal-stache"
  ],
  "directories": {
   "lib": "src"
  }
 },
 "dependencies": {
  "can-component": "^4.0.2",
  "can-define": "^2.0.1",
  "can-stache": "^4.1.0",
  "can-view-autorender": "^4.0.0",
  "cssify": "^0.6.0",
  "steal-css": "^1.2.5",
  "steal-less": "^1.2.2",
  "steal-platform": "^0.0.4",
  "steal-stache": "^4.0.1",
  "steal-tools": "^1.6.0",
  "can-connect": "^2.0.2",
  "can-route": "^4.1.0",
  "can-route-pushstate": "^4.0.1",
  "can-set": "^1.5.1",
  "can-stache-route-helpers": "<2.0.0",
  "can-zone": "^0.6.13",
  "done-autorender": "^2.0.0",
  "done-component": "^2.0.0",
  "done-css": "^3.0.2",
  "done-serve": "^2.0.0",
  "generator-donejs": "^2.1.0",
  "steal": "^1.6.5",
  "jquery": "^3.3.1"
 },
 "devDependencies": {
  "can-fixture": "^2.0.2",
  "chai": "^4.2.0",
  "chance": "^1.0.16",
  "done-serve": "^1.0.0",
  "donejs-cli": "^2.0.0",
  "eslint-config-tucows": "^1.0.7",
  "funcunit": "^3.2.0",
  "generator-donejs": "^1.0.0",
  "sinon": "^4.5.0",
  "steal": "^1.12.5",
  "steal-mocha": "^2.0.0",
  "steal-qunit": "^1.0.1",
  "steal-tools": "^1.11.3",
  "testee": "^0.7.0",
  "can-debug": "^1.0.0",
  "steal-conditional": "^1.0.0"
 },
 "license": "MIT",
 "bugs": {
  "url": "https://github.com/tucows/donejs-carousel-plugin/issues"
 }
}
